{{#each this}}
<div class="wrapper">
    <div class="content">
        <h1 class="country__name">{{ name }}</h1>
        <p class="country__capital">Capital: {{ capital }}</p>
        <p class="country__population">Population: {{ population }}</p>
        <p>Languages:
        <ul class="country__languages">
            {{#each languages}}
            <li>{{name}}</li>
            {{/each}}
        </ul>
        </p>
    </div>
    <img src="{{ flag }}" alt="" class="country__flag">
</div>

{{/each}}